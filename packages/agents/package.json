{
  "name": "@syner/agents",
  "version": "0.1.0",
  "type": "module",
  "description": "A toolkit to create/manage agents in the Syner OS",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "example:agent": "tsx --watch example/iac-engineer.ts",
    "example:supervisor": "tsx example/head-devops.ts",
    "example:validation": "tsx example/validation-example.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [],
  "author": {
    "name": "Syner OS",
    "email": "support@synerops.com",
    "url": "https://synerops.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synerops/syner.git",
    "directory": "packages/agents"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.30",
    "ai": "^5.0.44",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@syner/eslint-config": "workspace:*",
    "@syner/tsconfig": "workspace:*",
    "@types/node": "^20.19.15",
    "eslint": "^9.35.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
