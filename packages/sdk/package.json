{
  "name": "@syner/sdk",
  "version": "0.0.0-dev.1",
  "description": "Syner Agentic Framework SDK - Build autonomous systems",
  "type": "module",
  "private": false,
  "sideEffects": false,
  "license": "MIT",
  "prettier": "@syner/prettier-config",
  "author": {
    "name": "SynerOps, Inc",
    "email": "info@synerops.com",
    "url": "https://synerops.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "import": "./dist/actions.js",
      "require": "./dist/actions.cjs"
    },
    "./checks": {
      "types": "./dist/checks.d.ts",
      "import": "./dist/checks.js",
      "require": "./dist/checks.cjs"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "import": "./dist/context.js",
      "require": "./dist/context.cjs"
    },
    "./loop": {
      "types": "./dist/loop.d.ts",
      "import": "./dist/loop.js",
      "require": "./dist/loop.cjs"
    },
    "./skills": {
      "types": "./dist/skills.d.ts",
      "import": "./dist/skills.js",
      "require": "./dist/skills.cjs"
    },
    "./system": {
      "types": "./dist/system.d.ts",
      "import": "./dist/system.js",
      "require": "./dist/system.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "echo 'SDK build disabled - using Bun when ready'",
    "dev": "echo 'SDK dev mode disabled'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore",
    "clean": "git clean -xdf .cache .next .turbo dist node_modules"
  },
  "devDependencies": {
    "@syner/eslint-config": "workspace:*",
    "@syner/prettier-config": "workspace:*",
    "@syner/ts-config": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
