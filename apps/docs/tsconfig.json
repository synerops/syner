{
  "extends": "@syner/ts-config/nextjs",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/.source": [
        ".source"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
